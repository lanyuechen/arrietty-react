        "type": "img",
        "ext": "jpg",
        "pathname": "/upload",
        "host": "localhost:3000",
        "protocol": "http"
      },
      "@icon": {
        "type": "file",
        "config": {
          "url": "http://localhost:3000/api/upload"
        }
      },
      "age": 19,
      "@age": {
        "type": "number"
      },
      "sex": "man",
