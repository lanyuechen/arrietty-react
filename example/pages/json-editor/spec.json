          {"key": "man", "name": "男"},
          {"key": "women", "name": "女"}
        ]
      },
      "city": "shanghai",
      "@city": {
        "type": "select",
        "data": [
          {"key": "beijing", "name": "北京"},
          {"key": "shanghai", "name": "上海"},
          {"key": "tianjin", "name": "天津"}
        ]
      },
      "hoby": ["reading", "running"],
      "@hoby": {
        "type": "checkbox",
        "data": [
          {"key": "reading", "name": "读书"},
          {"key": "running", "name": "跑步"},
          {"key": "sleeping", "name": "睡觉"}
        ]
      }
    }
